{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "cloudformation:*",
        "iam:*",
        "lambda:*",
        "logs:*",
        "s3:*"
      ],
      "Resource": [
        "arn:aws:cloudformation:us-east-1:*:stack/adaptive-iam-service-dev/*",
        "arn:aws:cloudformation:us-east-1:*:stack/adaptive-iam-service-dev-*/*",
        "arn:aws:iam::*:role/adaptive-iam-service-dev-*",
        "arn:aws:iam::*:role/*-adaptive-iam-service-dev-*",
        "arn:aws:lambda:us-east-1:*:function:adaptive-iam-service-dev-*",
        "arn:aws:lambda:us-east-1:*:layer:adaptive-iam-service-dev-*",
        "arn:aws:s3:::adaptive-iam-service-dev-*",
        "arn:aws:s3:::adaptive-iam-service-dev-*/*",
        "*"
      ]
    }
  ]
}